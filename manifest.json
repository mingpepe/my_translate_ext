{
  "manifest_version": 3,
  "name": "Translate Extension",
  "version": "1.0",
  "description": "Translate selected text using Google Translate",
  "permissions": ["contextMenus", "tabs", "activeTab"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "content_scripts": [
    {
      "matches": ["https://translate.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
